
<%- contentFor('hero') %>
<section class="hero">
    <div class="hero-content">
        <h2>All ABOUT us...</h2>
        <p>A good about. We are teamplayers.</p>

        <a href="/contact" class="btn btn-lg btn-primary">Drop a line guys.</a>
    </div>
</section>

<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>The Freakin Team</h1>

    <div class="row">
        <% for (user of users) { %>
            <div class="col-sm-4">
                <h2><%= user.name %>
                <img src="<%= user.avatar %>" class="img-responsive">
            </div>
        <% } %>
    </div>
</div>
